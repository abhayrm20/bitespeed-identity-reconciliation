{
  "name": "bitespeed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-prod": "node ./src/index.js",
    "clean": "del ./build/* ",
    "build": "npm run clean && tsc --pretty",
    "start-with-generate": "npm run prisma:generate && npm run prisma:migrate-deploy; ts-node-dev --poll src/index.ts",
    "start": "ts-node-dev --poll src/index.ts",
    "prisma:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate-dev": "npx prisma migrate dev --preview-feature --name=prisma_migration_$(date +'%s')",
    "prisma:studio-dev": "npx prisma studio --port 7000",
    "prisma:migrate-deploy": "npx prisma migrate deploy --schema=./src/prisma/schema.prisma"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "Abhay R Muni",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-bearer-token": "^2.4.0",
    "express-request-id": "^1.4.1",
    "express-validator": "^6.6.1",
    "json-stringify-safe": "^5.0.1",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.3.2",
    "mongoose-update-if-current": "^1.4.0",
    "morgan": "^1.10.0",
    "nats": "^2.15.1",
    "node-fetch": "^2.6.1",
    "prisma": "^4.16.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.17",
    "@types/express-request-id": "^1.4.1",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.0",
    "@types/uuid": "^9.0.2",
    "del-cli": "^5.0.0",
    "typescript": "^5.1.6"
  }
}